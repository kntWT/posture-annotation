{
	"name": "posture-annotation",
	"version": "0.0.1",
	"private": true,
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	},
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 5173",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@felte/reporter-svelte": "^1.1.11",
		"@smui-extra/accordion": "^7.0.0",
		"@smui/button": "^7.0.0",
		"@smui/card": "^7.0.0",
		"@smui/checkbox": "^7.0.0",
		"@smui/data-table": "^7.0.0",
		"@smui/dialog": "^7.0.0",
		"@smui/drawer": "^7.0.0",
		"@smui/icon-button": "^7.0.0",
		"@smui/layout-grid": "^7.0.0",
		"@smui/list": "^7.0.0",
		"@smui/paper": "^7.0.0",
		"@smui/select": "^7.0.0",
		"@smui/textfield": "^7.0.0",
		"@smui/tooltip": "^7.0.0",
		"@smui/top-app-bar": "^7.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"dotenv": "^16.4.5",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"felte": "^1.2.14",
		"p5": "^1.9.4",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.77.1",
		"smui": "^1.0.5",
		"smui-theme": "^7.0.0",
		"svelte": "^4.2.19",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.4.8",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@felte/validator-zod": "^1.0.17",
		"@smui/ripple": "^7.0.0",
		"p5-svelte": "^3.1.2",
		"svelte-material-ui": "^7.0.0",
		"zod": "^3.23.8"
	}
}
